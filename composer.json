{
    "name": "oxygen/theme",
    "description": "Theming for Oxygen",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "theme",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "c.chamberlain@iinet.net.au"
        }
    ],
    "require": {
        "php": ">=5.5.9",
        "illuminate/support": "~5.1.0"
    },
    "autoload": {
        "psr-4": {
            "Oxygen\\Theme\\": "src/"
        }
    },
    "extra": {
        "title": "Oxygen Theme",
        "readme": "oxygen.md",
        "changes": "changes.md",
        "icon": "icon.png",
        "providers": [
            {
                "name": "Theme",
                "class": "Oxygen\\Theme\\ThemeServiceProvider",
                "description": "Adds the Theming Framework"
            }
        ]
    }
}
