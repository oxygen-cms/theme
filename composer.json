{
    "name": "oxygen/theme",
    "description": "Theming for Oxygen",
    "license": "MIT",
    "keywords": [
        "cms",
        "framework",
        "theme",
        "OxygenExtension"
    ],
    "authors": [
        {
            "name": "Chris Chamberlain",
            "email": "chris@chamberlain.id.au"
        }
    ],
    "require": {
        "php": ">=7.4.0",
        "illuminate/support": "~6.0"
    },
    "autoload": {
        "psr-4": {
            "Oxygen\\Theme\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Oxygen\\Theme\\ThemeServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpstan/phpstan": "^0.12.18"
    }
}
